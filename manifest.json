{
  "manifest_version": 3,
  "name": "F1 Blocker",
  "version": "1.0.0",
  "description": "A chrome extension to block the F1 key",
  "action": {
    "default_popup": "popup.html",
    "default_title": "Enable or disable F1 blocking"
  },
  "permissions": [
    "storage",
    "notifications",
    "activeTab",
    "webNavigation"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_start"
    }
  ],
  "background": {
    "service_worker": "background.js"
  }
}

