{
  "manifest_version": 3,
  "name": "F1 Blocker",
  "version": "1.0.0",
  "description": "A chrome extension to block the F1 key",
  "action": {
    "default_popup": "popup.html",
    "default_title": "Enable or disable F1 blocking",
    "default_icon": {
      "16": "icon-16.svg",
      "48": "icon-48.svg",
      "128": "icon-128.svg"
    }
  },
  "permissions": [
    "storage",
    "webNavigation"
  ],
  "background": {
    "service_worker": "background.js"
  }
}

